{
    "name": "roadie-xx/dotenv",
    "description": "Loads environment variables from .env file to getenv(), $_ENV and $_SERVER.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "F.Michel",
            "email": "dev@devcoder.xyz"
        },
        {
            "name": "Robby van Zweeden"
        }
    ],
    "autoload": {
        "psr-4": {
            "RoadieXX\\": "src",
            "Test\\RoadieXX\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml --do-not-cache-result --colors=always --testdox",
        "test:coverage": "phpunit --configuration phpunit.xml --do-not-cache-result --colors=always  --testdox --coverage-clover build/logs/clover.xml --coverage-html build/coverage"
    },
    "require": {
        "php": ">=7.4",
        "another-vendor/package": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.3"
    }
}